          <TableColumn fx:id="colIdInstituto" prefWidth="92.0" text="ID Instituto" />
          <TableColumn fx:id="colNombreInstituto" prefWidth="148.0" text="Nombre Instituto" />
        </columns>
      </TableView>
      <Button fx:id="btnCrear" layoutX="80.0" layoutY="344.0" mnemonicParsing="false" onAction="#crearInstituto" prefHeight="25.0" prefWidth="114.0" text="Crear Instituto" />
