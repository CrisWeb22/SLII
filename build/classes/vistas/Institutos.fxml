      <VBox layoutX="30.0" layoutY="76.0" prefHeight="43.0" prefWidth="155.0">
         <children>
            <Label fx:id="lbIdInstituto" text="ID Instituto" />
            <TextField fx:id="txtIdInstituto" />
         </children>
      </VBox>
   </children>
</AnchorPane>
